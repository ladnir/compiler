[
	[let[calcWidth a b n][float float float int]
		[let [[width float] ]]

		[:= width [- b a]]
		[:= width [/ width n]]

		[return width]
	]

	[let[calcArea x area][float float float]

		[return[+ area [^ x 2]]]
	]

	[let[main][int]
		
		[let[[n int]
			[a float][b float][area float][width float][x float]
		]]
		
		[:= n 100]
		[:= b 1]

		[:= width [calcWidth a b n]]
		
		[while [< x b]
			[:= area [calcArea x area]]
			[:= x [+ x width]]
		]
		
		[stdout area]
	]

	[main]
]